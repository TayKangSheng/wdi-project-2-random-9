<div class="container">
  <h1>Which zine(s) would you like to add this photo to?</h1>
  <br>
  <form method="POST" action=<%= "/photos/" + photo + "?_method=PUT"%>>
    <% zine.forEach(function(zine) { %>
      <div class="checkbox">
      <label><input type="checkbox" value=<%=zine.name%> name=<%=zine.name%>><%= zine.name %></label>
      </div>
      <% }) %>
      <br>
      <button type="submit" class="btn btn-default">Submit</button>
  </form>
</div>
